{
	"name": "svalendar",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"devi": "NODE_TLS_REJECT_UNAUTHORIZED=0 vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"db:seed": "cd database && sqlite3 main.db < seed.sql",
		"db:shell": "cd database && sqlite3 main.db",
		"key": "tsx scripts/key.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^6.0.4",
		"@sveltejs/kit": "^2.53.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/jsonwebtoken": "^9.0.10",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"svelte": "^5.53.3",
		"svelte-check": "^4.4.3",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@fortawesome/free-regular-svg-icons": "^7.2.0",
		"@fortawesome/free-solid-svg-icons": "^7.2.0",
		"@libsql/client": "^0.17.0",
		"@skorotkiewicz/snowflake-id": "^1.0.1",
		"bcryptjs": "^3.0.3",
		"jsonwebtoken": "^9.0.3",
		"sql-template-tag": "^5.2.1",
		"svelte-fa": "^4.0.4",
		"zod": "^3.24.3"
	}
}
